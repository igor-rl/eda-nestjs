{
  "name": "eda_nestjs",
  "version": "0.0.1",
  "description": "Nestjs with Event Driven Architecture",
  "author": "Igor R Lage <[https://github.com/igor-rl]>",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:generate": "yarn typeorm migration:generate $npm_config_name -d src/nest-modules/database-module/typeorm-config.postgres.ts",
    "migration:create": "yarn typeorm migration:create $npm_config_name",
    "migration:run": "yarn typeorm migration:run -d src/nest-modules/database-module/typeorm-config.postgres.ts",
    "migration:run:prod": "yarn typeorm migration:run -d dist/nest-modules/database-module/typeorm-config.postgres.js",
    "migration:revert": "yarn typeorm migration:revert -d src/nest-modules/database-module/typeorm-config.postgres.ts",
    "migration:revert:prod": "yarn typeorm migration:revert -d dist/nest-modules/database-module/typeorm-config.postgres.js",
    "subscriber:create": "yarn typeorm subscriber:create $npm_config_name",
    "rabbitmq:consumers:dev": "npm run start:dev -- --entryFile cmd/rabbitmq",
    "rabbitmq:consumers:prod": "node dist/cmd/rabbitmq",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "test:e2e:watch": "jest --config ./test/jest-e2e.config.ts --watch",
    "typeorm": "ts-node ./node_modules/typeorm/cli"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^5.3.0",
    "@google-cloud/storage": "^7.7.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "chance": "^1.1.11",
    "class-transform": "^0.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.3.1",
    "eventemitter2": "^6.4.9",
    "joi": "^17.10.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-themes": "^1.2.30",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.84",
    "@swc/jest": "^0.2.29",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/chance": "^1.1.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  }
}
